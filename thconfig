encoding  utf-8
language ru

source main.th

export model -o output/cave.lox
export model -format survex -o output/cave.3d

layout normal
     symbol-set AUT
     doc-author "Paweł Krawczyk"
     doc-title "Суу-Акан"
     scale 1 400
     statistics explo all  
     statistics topo all
     statistics carto all
     statistics copyright all
     legend on
     map-comment "Сары Тала"
endlayout

export map -format pdf -layout normal -o output/cave.pdf
export map -o output/cave.kml
export map -o output/cave.xvi
